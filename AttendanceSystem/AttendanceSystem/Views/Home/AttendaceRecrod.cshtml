@model List<AttendanceSystem.Models.GetString>
@{
    ViewBag.Title = "出勤紀錄";
}

<meta http-equiv="refresh" content="5" />
<h2>出勤紀錄</h2>
<table class="table table-bordered">
    <thead><tr><th style="text-align:center;">RFID_UID</th><th style="text-align:center;">使用者名稱</th><th style="text-align:center;">時間</th><th style="text-align:center;">狀態</th></tr></thead>
    <tbody>
        @foreach (var pd in this.Model) /* 使用foreach將所有商品列成表格元素 */
        {
            <tr>
                <td>@pd.RFID_UID</td>
                <td>@pd.UserName</td>

                <td>@pd.Date</td>
                <td>@pd.Status</td>

            </tr>
        }
    </tbody>
</table>
